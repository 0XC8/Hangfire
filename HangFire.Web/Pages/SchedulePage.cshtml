@* Generator: Template TypeVisibility: Internal *@
@using HangFire.Web.Pages
@inherits WebTemplateBase
              
@{
    Layout = new LayoutPage()
        {
            Context = Context,
            Title = "Schedule"
        };
}

@{
    var scheduledJobs = Storage.Schedule();
}

@if (scheduledJobs.Count == 0)
{
    <div class="alert alert-success">
        Нет ни одного запланированного задания.
    </div>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Timestamp</th>
                <th>Type</th>
                <th>Queue</th>
                <th>Args</th>
            </tr>
        </thead>
        @foreach (var job in scheduledJobs)
        {
            <tr>
                <td>@job.TimeStamp</td>
                <td>@job.Type</td>
                <td>@job.Queue</td>
                <td>@job.Args</td>
            </tr>
        }
    </table>
}