@* Generator: Template TypeVisibility: Internal *@
@using HangFire.Web.Pages
@inherits WebTemplateBase
              
@{
    Layout = new LayoutPage()
        {
            Context = Context,
            Title = "Queues"
        };
}

<table class="table">
    <thead>
        <tr>
            <th>Queue</th>
            <th>Length</th>
            <th>Servers</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var queue in HangFireApi.Queues())
        {
            <tr>
                <td>@queue.Name</td>
                <td>@queue.Length</td>
                <td>@string.Join(", ", queue.Servers)</td>
            </tr>
        }
    </tbody>
</table>