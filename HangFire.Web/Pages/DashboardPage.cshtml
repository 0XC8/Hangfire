@* Generator: Template TypeVisibility: Internal *@
@using HangFire.Web.Pages
@using ServiceStack.Text
@inherits WebTemplateBase
              
@{
    Layout = new LayoutPage
        {
            Context = Context,
            Title = "Dashboard"
        };
}

<h3>Realtime graph</h3>
<div id="realtime"></div>

@{
    // TODO: get rid of this. We have no right to change the library globals.
    JsConfig.DateHandler = JsonDateHandler.ISO8601;
}

<h3>History graph</h3>
<div id="history" 
     data-succeeded="@JsonSerializer.SerializeToString(HangFireApi.HourlySucceededJobs())" 
     data-failed="@JsonSerializer.SerializeToString(HangFireApi.HourlyFailedJobs())">
</div>